    $(document).ready(function () {
        $("#video-audio").click(function () {
            if (isPlaying) {
                myAudio.pause()
                $(".pause-audio").html('<img src="/images/ipl/mute.png" width="" height="" alt="" />');
            } else {
                myAudio.play();
                $(".pause-audio").html('<img src="/images/ipl/unmute.png" width="" height="" alt="" />');
            }
        });        
    });
    
    myAudio.onplaying = function () {
        isPlaying = true;
    };
    myAudio.onpause = function () {
